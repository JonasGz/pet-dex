{
  "name": "pet-dex-frontend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:coverage": "vitest --ui --coverage",
    "build": "vite build --emptyOutDir",
    "build-storybook": "storybook build",
    "code-style": "concurrently \"pnpm lint\" \"pnpm stylelint\" \"pnpm prettier\"",
    "commit": "git-cz",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js,.ts,.tsx . --report-unused-disable-directives --max-warnings 0 --fix && stylelint '**/*.{css,scss}' --fix",
    "prepare": "husky install",
    "prettier": "prettier . --check --ignore-unknown",
    "prettier:fix": "prettier . --write --ignore-unknown",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "stylelint": "stylelint \"**/*.scss\"",
    "mock-api": "json-server db.json --routes routes.json"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-web": "0.44.0",
    "concurrently": "8.2.2",
    "dayjs": "^1.11.13",
    "firebase": "11.7.1",
    "pet-dex-utilities": "^1.0.1",
    "reset-css": "^5.0.2",
    "vanilla-routing": "0.0.4",
    "vite": "^5.2.11",
    "vite-jsconfig-paths": "2.0.1",
    "vite-plugin-pwa": "^0.19.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "19.8.1",
    "@faker-js/faker": "8.4.1",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/html": "8.6.12",
    "@storybook/html-vite": "8.6.12",
    "@storybook/test": "8.6.12",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/user-event": "14.6.1",
    "@vitest/coverage-v8": "1.5.3",
    "@vitest/ui": "1.5.3",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-jsconfig": "1.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-testing-library": "6.2.2",
    "eslint-plugin-vitest": "0.5.4",
    "eslint-plugin-vitest-globals": "1.5.0",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "json-server": "0.17.4",
    "lint-staged": "^15.2.2",
    "prettier": "^3.5.3",
    "reset-css": "^5.0.2",
    "sass": "^1.88.0",
    "storybook": "8.6.12",
    "stylelint": "^16.19.1",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-sass-guidelines": "11.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.12.0",
    "vitest": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "pnpm@8.15.4+sha512.0bd3a9be9eb0e9a692676deec00a303ba218ba279d99241475616b398dbaeedd11146f92c2843458f557b1d127e09d4c171e105bdcd6b61002b39685a8016b9e"
}
